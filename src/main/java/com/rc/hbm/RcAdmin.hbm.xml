<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.rc.model.RcAdmin" table="rc_admin" schema="rc">
        <id name="userId" type="java.lang.Integer" column="adm_id"/>
        <property name="userName" type="java.lang.String" column="adm_name"/>
        <property name="userPwd" type="java.lang.String" column="adm_pwd"/>
    </class>
</hibernate-mapping>